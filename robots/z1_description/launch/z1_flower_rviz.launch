<launch>

    <!-- <arg name="user_debug" default="false"/> -->
    <arg name="UnitreeGripperYN" default="true"/>
    
    <param name="robot_description" command="$(find xacro)/xacro --inorder '$(find z1_description)/xacro/robot_flower.xacro' UnitreeGripper:=$(arg UnitreeGripperYN)"/>

    <!-- for higher robot_state_publisher average rate-->
    <!-- <param name="rate" value="1000"/> -->

    <!-- send fake joint values -->
    <!-- <node pkg="joint_state_publisher_gui" type="joint_state_publisher_gui" name="joint_state_publisher_gui">
        <param name="use_gui" value="TRUE"/>
    </node> -->


    <node pkg="robot_state_publisher" type="robot_state_publisher" name="robot_state_publisher">
        <remap from="/joint_states" to="/arm_controller/joint_states"/>
        <param name="publish_frequency" type="double" value="1000.0"/>
    </node> 

    <node pkg="tf2_ros" type="static_transform_publisher" name="arm2zed"
        args="-0.39865 0. -0.02174 0 -2.093 0 link02 zed_base"/>
    <node pkg="rviz" type="rviz" name="rviz" respawn="false" output="screen"
        args="-d $(find z1_description)/launch/setting_flower.rviz"/>

</launch>
